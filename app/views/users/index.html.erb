<h1><%= current_user.full_name %></h1>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan=2>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= link_to 'Edit', edit_user_registration_path(user) %></td>
        <td><%= link_to 'View', user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Log out', destroy_user_session_path, method: :delete %>