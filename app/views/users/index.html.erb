<h1><%= current_user.full_name %></h1>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan=3>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td>View</td>
        <td>Edit</td>
        <td>Delete</td>
      </tr>
    <% end %>
  </tbody>
</table>